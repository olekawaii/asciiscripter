---
    ascii art of a dragon
    by olekawaii
<o>

extend : fn frames frames
  seq $1 take 1 reverse $1
end

main : frames
  join move 25 -3 title join 
    skip 3 move_y 7 join skip 6 move_x 30 fire_right move_x -19 fire_left 
    join
      move 1 -1 slow 2 join
        dragon_foot_left
        move_x 12 skip 1 dragon_foot_right
      join
        join
           side_heads
          join slow 3 move 7 -4 dragon_tail dragon_body
        move -10 1 join 
          skip 2 wing_left 
          move_x 29 wing_right
end

