side_heads : frames
  move_y 5 join
    skip 6 move_x -9 seq 
      move -1 2 dragon_blow_left 
      dragon_roar_left
    move_x 16 seq 
      move_y 2 dragon_blow_right 
      dragon_roar_right
end

