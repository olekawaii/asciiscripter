side_heads : frames
  do 7 shift north join 
    join
      do 9 shift west do 6 rotate seq dragon_blow_left dragon_roar_left
      do 16 shift east seq dragon_blow_right dragon_roar_right
    join 
      do 9 rotate do 30 shift east fire_right 
      do 3 rotate do 19 shift west fire_left
end

